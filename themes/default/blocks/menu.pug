-
  var menu = {
    'about company': '#about',
    'our products': '#products',
    'markets': '#markets',
    'our partners': '#partners',
    'contacts': '#contacts',
  };

nav
  ul
    each val, key in menu
      li
        if selected === key
          a.selected(href=val, title=key)= key
        else
          a(href=val, title=key)= key